{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "MongoDb": {
        "ConnectionString": "mongodb://admin:shancrys2024@localhost:27017/shancrys?authSource=admin",
        "DatabaseName": "shancrys",
        "MaxConnectionPoolSize": 100,
        "MinConnectionPoolSize": 10,
        "ServerSelectionTimeout": "00:00:05",
        "ConnectTimeout": "00:00:10"
    },
    "JwtSettings": {
        "SecretKey": "your-super-secret-key-change-in-production-min-32-chars-long",
        "Issuer": "shancrys-api",
        "Audience": "shancrys-clients",
        "ExpirationMinutes": 60,
        "RefreshTokenExpirationDays": 7
    },
    "RabbitMQ": {
        "HostName": "localhost",
        "Port": 5672,
        "UserName": "guest",
        "Password": "guest"
    },
    "Stripe": {
        "SecretKey": "sk_test_your_key_here",
        "PublishableKey": "pk_test_your_key_here",
        "WebhookSecret": "whsec_your_webhook_secret",
        "Currency": "brl",
        "EnableTestMode": true
    },
    "AzureStorage": {
        "ConnectionString": "UseDevelopmentStorage=true",
        "ContainerName": "shancrys-models"
    },
    "Serilog": {
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "System": "Warning"
            }
        }
    }
}